//14.Reverse an array
//Input: arr = [1, 4, 3, 2, 6, 5]   Output: [5, 6, 2, 3, 4, 1]


class Solution {
    public void reverseArray(int arr[]) {
        int i=0;
        int j=arr.length-1;
        int temp=0;
        while(i<j){
            temp=arr[i];
            arr[i]=arr[j];
            arr[j]=temp;
            i++;
            j--;
        }
    }
}
